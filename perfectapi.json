{	"exports": "PaasHost",
	"signature": [
		{ 
			"name": "publishApp",
			"synopsis": "Publishes an app",
			"description": "",
			"verb": "POST",
			"parameter": {"name":"AppName", "required":true, "description":"unique name of the app"},
			"options": [
				{"option":"file", "required":true, "description":"zip file containing the contents of the service to install"},
				{"option":"path", "required":true, "description":"url path on which the service will listen"}
			]
		}
	], 
	"path": "paas/host",
	"environment": [
		{"parameter": "SERVICE_REGISTRY_URL", "required":true, "description":"Full url of service registry endpoint"}
	]
}